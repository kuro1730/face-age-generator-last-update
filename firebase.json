{
  "hosting": {
    "headers": [{
      "source": "**",
      "headers": [{
          "key": "Cache-Control",
          "value": "max-age=300"
      },
    {     "key": "Access-Control-Allow-Origin",
          "value": "*"}]
  }],
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    
    ]
  }
}
